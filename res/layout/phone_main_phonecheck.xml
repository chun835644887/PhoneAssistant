<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <include layout="@layout/title_activity" />
<LinearLayout 
    android:id="@+id/line1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
    <ProgressBar
        android:id="@+id/phone_main_phonecheck_battery"
        style="@android:style/Widget.ProgressBar.Horizontal"
        android:layout_width="180dp"
        android:layout_height="80dp"
        android:layout_marginLeft="20dp"
        
        android:layout_marginTop="40dp" 
        android:clickable="true"
        android:layout_weight="1"
        android:background="@drawable/battery_progress"/>

    <ProgressBar
        android:id="@+id/phone_main_phonecheck_battery1"
        style="@android:style/Widget.ProgressBar.Horizontal"
        android:layout_width="10dp"
        android:layout_height="42dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/battery_progress" />

   <TextView 
       android:id="@+id/phone_main_check_batterytext"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="dddd"
       android:layout_gravity="center_vertical"/>"
</LinearLayout>
    <RelativeLayout
        android:id="@+id/relative1"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_below="@id/line1"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp" >

        <ImageView
            android:id="@+id/phone_main_phonecheck_device"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/notification_information_progress_green"
            android:src="@drawable/setting_info_icon_version" />

        <TextView
            android:id="@+id/phone_main_phonecheck_phonename"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="3dp"
            android:layout_toRightOf="@id/phone_main_phonecheck_device"
            android:singleLine="true"
            android:text="dddd"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/phone_main_phonecheck_phongversion1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/phone_main_phonecheck_phonename"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="3dp"
            android:layout_toRightOf="@id/phone_main_phonecheck_device"
            android:singleLine="true"
            android:text="ddddd"
            android:textSize="15sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relative2"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_below="@id/relative1"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp" >

        <ImageView
            android:id="@+id/phone_main_phonecheck_runMemory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/notification_information_progress_red"
            android:src="@drawable/setting_info_icon_space" />

        <TextView
            android:id="@+id/phone_main_phonecheck_phoneallrun"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="3dp"
            android:layout_toRightOf="@id/phone_main_phonecheck_runMemory"
            android:singleLine="true"
            android:text="dddd"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/phone_main_phonecheck_phongversion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/phone_main_phonecheck_phoneallrun"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="3dp"
            android:layout_toRightOf="@id/phone_main_phonecheck_runMemory"
            android:singleLine="true"
            android:text="ddddd"
            android:textSize="15sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relative3"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_below="@id/relative2"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp" >

        <ImageView
            android:id="@+id/phone_main_phonecheck_phonecpu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/notification_information_progress_yellow"
            android:src="@drawable/setting_info_icon_cpu" />

        <TextView
            android:id="@+id/phone_main_phonecheck_cpuname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="3dp"
            android:layout_toRightOf="@id/phone_main_phonecheck_phonecpu"
            android:singleLine="true"
            android:text="dddd"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/phone_main_phonecheck_cpucount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/phone_main_phonecheck_cpuname"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="3dp"
            android:layout_toRightOf="@id/phone_main_phonecheck_phonecpu"
            android:singleLine="true"
            android:text="ddddd"
            android:textSize="15sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relative4"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_below="@id/relative3"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp" >

        <ImageView
            android:id="@+id/phone_main_phonecheck_camera"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/notification_information_progress_green"
            android:src="@drawable/setting_info_icon_camera" />

        <TextView
            android:id="@+id/phone_main_phonecheck_phonefen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="3dp"
            android:layout_toRightOf="@id/phone_main_phonecheck_camera"
            android:singleLine="true"
            android:text="dddd"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/phone_main_phonecheck_camerafen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/phone_main_phonecheck_phonefen"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="3dp"
            android:layout_toRightOf="@id/phone_main_phonecheck_camera"
            android:singleLine="true"
            android:text="ddddd"
            android:textSize="15sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relative5"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_below="@id/relative4"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp" >

        <ImageView
            android:id="@+id/phone_main_phonecheck_jidai"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/notification_information_progress_red"
            android:src="@drawable/setting_info_icon_root" />

        <TextView
            android:id="@+id/phone_main_phonecheck_jidaiversion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="3dp"
            android:layout_toRightOf="@id/phone_main_phonecheck_jidai"
            android:singleLine="true"
            android:text="dddd"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/phone_main_phonecheck_jidairoot"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/phone_main_phonecheck_jidaiversion"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="3dp"
            android:layout_toRightOf="@id/phone_main_phonecheck_jidai"
            android:singleLine="true"
            android:text="ddddd"
            android:textSize="15sp" />
    </RelativeLayout>

</RelativeLayout>